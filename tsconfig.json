{
	"compilerOptions": {
		"module": "ESNext",
		"target": "ES6",
		"moduleResolution": "node",
		"esModuleInterop": true,
		"resolveJsonModule": true,
		"allowSyntheticDefaultImports": true,
		"strict": true,
		"skipLibCheck": true
	},
	"include": ["**/*.ts", "**/*.svelte"],
	"extends": "./.svelte-kit/tsconfig.json"
}
